    "recompose": "^0.30.0",
    "utility-types": "3.4.1"
  },
  "scripts": {
    "analyze": "source-map-explorer '../server/ui/static/js/*.js'",
    "bootstrap": "cd ../../ && yarn bootstrap",
    "prebuild": "yarn saveTypes",
    "build": "NODE_PATH=src react-scripts build && rm -rf ../server/ui && mv build ../server/ui",
    "format": "cd ../../ && yarn format",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "saveSchema": "yarn saveSchema:json && yarn saveSchema:graphql",
    "saveSchema:json": "apollo schema:download --endpoint=http://localhost:30001/graphql graphql-schema.json",
    "saveSchema:graphql": "./scripts/saveSchema.js > graphql-schema.graphql",
    "saveTypes": "yarn saveTypes1 && yarn saveTypes2",
    "saveTypes1": "gql2ts -n Acnw -o ./typings/Acnw.d.ts graphql-schema.json",
    "saveTypes2": "apollo client:codegen --target typescript --outputFlat ./src/__generated__ && rm -f src/__generated__/globalTypes.ts",
    "start": "NODE_PATH=src PORT=30000 react-scripts start",
    "test": "CI=true NODE_PATH=src react-scripts test",
    "test:watch": "NODE_PATH=src react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
