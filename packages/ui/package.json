{
  "name": "ui",
  "version": "0.1.0",
  "description": "AmberCon NW v2 site/UI",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "private": true,
  "proxy": "http://localhost:30001/",
  "devDependencies": {

  },

  "scripts": {
    "analyze": "source-map-explorer '../server/ui/static/js/*.js'",
    "bootstrap": "cd ../../ && yarn bootstrap",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "yarn codegen --watch",
    "build": "react-scripts build && rm -rf ../server/ui && mv build ../server/ui",
    "format": "cd ../../ && yarn format",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "start": "concurrently --kill-others --names 'codegen,CRA' --prefix-colors 'blue,yellow' 'yarn codegen:watch' 'REACT_REFRESH=false PORT=30000 react-scripts start'",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "typesync": "typesync"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
