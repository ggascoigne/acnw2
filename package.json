  "scripts": {
    "analyze": "source-map-explorer 'build/js/*.js'",
    "setup": "yarn db:migrate:up && node scripts/prepareGraphql.js && yarn codegen",
    "prebuild": "yarn setup",
    "prestart": "yarn setup",
    "build": "react-app-rewired build",
    "clean:install": "rm -rf node_modules && yarn && yarn audit",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:clean": "yarn db:start && ./scripts/createCleanDb.js",
    "db:import": "yarn db:start && ./scripts/importV1ToLocal.js",
    "db:migrate:down": "knex-migrate --cwd ./support --knexfile ./knexfile.js down",
    "db:migrate:up": "knex-migrate --cwd ./support --knexfile ./knexfile.js up",
    "db:setup": "yarn db:clean && yarn db:migrate:run ",
    "db:start": "docker-compose up -d && ./scripts/wait-for-postgres.sh -c acnw-postgres",
    "debug": "now dev --listen 30000",
    "format": "prettier --no-color --loglevel warn --write '{src,api,scripts,types}/**/*.{js,jsx,ts,tsx,css,scss,json,graphql}'",
    "lint": "eslint '{src,api,scripts,support,types}/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "patch-package",
    "start": "BROWSER=none react-app-rewired start",
    "test": "react-app-rewired test",
    "typesync": "typesync"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "jsxSingleQuote": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --no-color --write",
      "yarn lint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  },
  "resolutions": {
    "typescript": "3.9.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
